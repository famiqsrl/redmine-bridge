{
    "name": "famiq/redmine-bridge",
    "description": "Redmine bridge integration for internal systems (Redmine + RedmineUP Helpdesk/CRM)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Famiq\\RedmineBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famiq\\RedmineBridge\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "nyholm/psr7": "^1.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5"
    }
}
