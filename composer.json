{
    "name": "famiq/ad-user",
    "description": "Famiq Active Directory User integration package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Famiq\\ActiveDirectoryUser\\": ""
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Famiq\\ActiveDirectoryUser\\FamiqADUserServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/famiq/redmine-bridge"
        }
    ],
    "require": {
        "php": ">=8.2",
        "directorytree/ldaprecord-laravel": "^3.4",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "famiq/redmine-bridge": "*"
    }
}
