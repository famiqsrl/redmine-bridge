{
    "name": "famiq/redmine-bridge",
    "description": "Redmine bridge integration for internal systems (Redmine + RedmineUP Helpdesk/CRM)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Famiq\\RedmineBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famiq\\RedmineBridge\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "nyholm/psr7": "^1.7",
        "symfony/console": "^6.0|^7.0",
        "symfony/http-client": "^6.0|^7.0",
        "symfony/options-resolver": "^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Famiq\\RedmineBridge\\Laravel\\RedmineBridgeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "php-http/mock-client": "^1.6",
        "php-http/discovery": "^1.17",
        "doctrine/dbal": "^3.6",
        "illuminate/database": "^9.0|^10.0"
    },
    "suggest": {
        "doctrine/dbal": "Required for Symfony idempotency store.",
        "illuminate/database": "Required for Laravel idempotency store.",
        "illuminate/support": "Required for Laravel service provider integration."
    }
}
